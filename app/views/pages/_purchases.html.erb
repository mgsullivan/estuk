<table class="table table-condensed">
	<thead>
		<tr>
			<th>Name</th>
			<th>sold</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% @purchases.each do |purchase| %>
			<tr>
				<td><%= link_to purchase.book.name, pickup_path(purchase.guid) %></td>
				<td><%= time_ago_in_words(purchase.created_at) %> ago</td>
				
			</tr>
		<% end %> 
	</tbody>
</table>